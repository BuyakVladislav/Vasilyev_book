#include <iostream>

using namespace std;

// Напишите программу, в которой создается массив и заполняется четными натуральными числами

int main() {
	int a, b, c;
	cin >> a >> b >> c;

	if (a % 2 == 0 and b % 2 == 0 and c % 2 == 0)
	{
		int arr[] = { a, b, c };
		cout << arr[0] << " " << arr[1] << " " << arr[2] << endl;
	}
	else
	{
		cout << "Error!!!" << endl;
	}
	
}
