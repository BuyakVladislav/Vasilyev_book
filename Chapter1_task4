// Написать программу для отображения в консоли чисел Фибоначчи (при условии, что первые два числа в последовательности равны единице, 
// а каждое последующее равняется сумме двух предыдущих). Количество выводимых чисел вводится с клавиатуры.

#include <iostream>
#include <cstdlib>

using namespace std;

int main() {
	int n, a = 1, b = 1, c = 0, step = 0; // n - количество чисел, a и b - первые числа, c - третье число, step  - шаг цикла;
	cin >> n; // задаем количество выводимых чисел
	cout << a << "  " << b << "  "; // вывод двух единиц в начале
	while (step + 3 <= n) {   // в условии цикла прописал + 3, т.к. мы уже вывели первые два числа в консоль до начала цикла
		c = a + b; // третье и последующие числа Фибоначчи
		cout << c << "  ";
		a = b; 
		b = c;
		step++;
	}

	return 0;
}
